<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodrunner">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CatManagement.Activity.DinnerAdmin"></activity>
        <activity android:name=".CatManagement.Activity.LunchAdmin" />
        <activity android:name=".CatManagement.Activity.BreakfastAdmin" />
        <activity android:name=".CatManagement.Activity.Dinner" />
        <activity android:name=".CatManagement.Activity.Lunch" />
        <activity android:name=".CatManagement.Activity.Breakfast" />
        <activity android:name=".CatManagement.Activity.DeleteItem" />
        <activity android:name=".CatManagement.Activity.AddItem" />
        <activity android:name=".CartManagement.Model.ui.ListShoppingList"></activity>
        <activity android:name=".CartManagement.Model.ui.CreateShoppingList" />
        <activity android:name=".CartMainActivity" />
        <activity android:name=".Category" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>